<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alpha.kooi.alpharandom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.alpha.kooi.alpharandom.StartScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.alpha.kooi.alpharandom.InitialSetup"
            android:label="@string/title_activity_initial_setup"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.alpha.kooi.alpharandom.OptionsScreen"
            android:theme="@style/AppTheme" />
        <activity android:name="com.alpha.kooi.alpharandom.ResultScreen" />
        <activity android:name="com.alpha.kooi.alpharandom.EliminationScreen" />
        <activity android:name="com.alpha.kooi.alpharandom.SaveScreen"></activity>

    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>